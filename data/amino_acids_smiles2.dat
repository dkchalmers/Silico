#############################################################################################
#  Standard  amino  acids                                                                   #
#############################################################################################

# Alanine
CC(N)C=O			Alanine  		ALA   CB   CA   N    C   O 

#  Arginine
N=C(N)NCCCC(N)C=O		Arginine  		ARG   N1   CZ   N2  NE   CD   CG   CB   CA   N    C    O

# Asparagine
NC(=O)CC(N)C=O			Asparigine  		ASN   ND2  CG   OD1  CB   CA   N    C    O

# Aspartic  acid
OC(=O)CC(N)C=O			Aspartic_acid  		ASP   OD2  CG   OD1  CB   CA   N    C    O

# Cysteine
SCC(N)C=O			Cysteine  		CYS   SG   CB   CA   N    C    O

# Glycine
O=CCN				Glycine  		GLY   O    C    CA   N

# Histidine

# Glutamine
NC(=O)CCC(N)C=O			Glutamine  		GLN   NE2  CD   OE1  CG   CB   CA   N    C    O

# Glutamic  acid
O-C(=O)CCC(N)C=O		Glutamic_acid  		GLU   OE2  CD   OE1  CG   CB   CA   N    C    O

# Isoleucine
CCC(C)C(N)C=O			Isoleucine  		ILE   CD1  CG1  CB   CG2  CA   N    C    O

# Leucine
CC(C)CC(N)C=O			Leucine  		LEU   CD2  CG   CD1  CB   CA   N    C    O

# Methionine
CSCCC(N)C=O			Methionine  		MET   CE   SD   CG   CB   CA   N    C    O

# Phenylalanine
O=CC(Cc1ccccc1)N		Phenylalanine  		PHE   O    C    CA   CB   CG   CD1  CE1  CZ   CE2  CD2  N

# Proline
O=CC1CCCN1			Proline  		PRO   O    C    CA   CB   CG   CD   N

# Serine
OCC(N)C=O			Serine  		SER   OG   CB   CA   N    C    O

# Threonine
CC(O)C(N)C=O			Threonine  		THR   CG2  CB   OG1  CA   N    C    O

# Tryptophan
O=CC(CC1=CNc2ccccc12)N		Tryptophan  		TRP   O    C    CA   CB   CG   CD1  NE1  CE2  CZ2  CH2  CZ3  CE3  CD2  N

# Tyrosine
Oc1ccc(CC(N)C=O)cc1		Tyrosine  		TYR   OH   CZ   CE2  CD2  CG   CB   CA   N    C    O    CD1  CE1

# Valine
CC(C)C(N)C=O			Valine  		VAL   CG2  CB   CG1  CA   N    C    O

